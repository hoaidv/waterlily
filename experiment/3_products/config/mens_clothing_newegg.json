{
  "category": "Men's Clothing",
  "website": "newegg",
  "search_query": "men clothing shirts",
  "listing_selectors": {
    "product_container": "div.item-cell",
    "product_link": "a.item-title"
  },
  "product_selectors": {
    "title": [
      {"selector": "h1.product-title"}
    ],
    "price": [
      {"selector": "li.price-current"}
    ]
  },
  "attributes": {
    "brand": {
      "method": "regex",
      "pattern": "(Nike|Adidas|Under Armour|Puma|Reebok|New Balance|Champion)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "clothing_type": {
      "method": "regex",
      "pattern": "(T-Shirt|Shirt|Polo|Hoodie|Jacket|Pants|Shorts)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "size": {
      "method": "regex",
      "pattern": "\\b(XS|S|M|L|XL|XXL|XXXL|2XL|3XL)\\b",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "color": {
      "method": "regex",
      "pattern": "(Black|White|Blue|Red|Green|Gray|Grey|Navy)",
      "source": "title",
      "flags": "IGNORECASE"
    }
  }
}

