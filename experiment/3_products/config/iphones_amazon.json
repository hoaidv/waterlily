{
  "category": "iPhones",
  "website": "amazon",
  "search_query": "iPhone cell phone",
  "listing_selectors": {
    "product_container": "div[data-component-type='s-search-result']",
    "product_link": "h2 a",
    "fallback_containers": [
      "div.s-result-item",
      "div[data-asin]"
    ]
  },
  "product_selectors": {
    "title": [
      {"selector": "span#productTitle"},
      {"selector": "h1.a-size-large"}
    ],
    "price": [
      {"selector": "span.a-price span.a-price-whole"},
      {"selector": "span[data-a-color='price']"}
    ],
    "rating": [
      {"selector": "span.a-icon-alt", "regex": "(\\d+\\.?\\d*)"}
    ],
    "images": [
      {"selector": "img#landingImage", "attribute": "src"}
    ]
  },
  "attributes": {
    "brand": {
      "method": "regex",
      "pattern": "(Apple|iPhone)",
      "source": "title",
      "default": "Apple"
    },
    "model": {
      "method": "regex",
      "pattern": "(iPhone\\s*\\d+[a-zA-Z]*(?:\\s*Pro\\s*Max)?(?:\\s*Plus)?)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "storage": {
      "method": "regex",
      "pattern": "(\\d+)\\s*GB",
      "source": "title",
      "fallback": {
        "method": "selector",
        "selector": "span.selection"
      }
    },
    "color": {
      "method": "regex",
      "pattern": "(Black|White|Blue|Red|Green|Purple|Pink|Gold|Silver|Gray|Grey|Titanium|Starlight|Midnight|Graphite|Sierra Blue|Alpine Green|Rose Gold|Space Gray|Space Grey|Jet Black|Product Red)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "condition": {
      "method": "regex",
      "pattern": "(New|Renewed|Refurbished|Used)",
      "source": "title",
      "flags": "IGNORECASE",
      "default": "New"
    },
    "carrier": {
      "method": "regex",
      "pattern": "(Unlocked|AT&T|Verizon|T-Mobile|Sprint)",
      "source": "title",
      "flags": "IGNORECASE"
    }
  }
}

