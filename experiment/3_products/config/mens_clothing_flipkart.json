{
  "category": "Men's Clothing",
  "website": "flipkart",
  "search_query": "men clothing shirts",
  "listing_selectors": {
    "product_link": "a[href*='/p/']"
  },
  "product_selectors": {
    "title": [
      {"selector": "span.B_NuCI"},
      {"selector": "h1.yhB1nd"}
    ],
    "price": [
      {"selector": "div._30jeq3"}
    ],
    "rating": [
      {"selector": "div._3LWZlK"}
    ]
  },
  "attributes": {
    "brand": {
      "method": "regex",
      "pattern": "(Allen Solly|Peter England|Van Heusen|Arrow|Louis Philippe|Park Avenue|Raymond|Roadster|HRX|Puma|Adidas|Nike|Reebok|U.S. Polo Assn|Flying Machine|Wrangler|Lee|Levi's|Jack & Jones|Pepe Jeans|Being Human)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "clothing_type": {
      "method": "regex",
      "pattern": "(T-Shirt|Shirt|Polo|Hoodie|Jacket|Jeans|Pants|Shorts|Sweater|Sweatshirt|Kurta)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "size": {
      "method": "regex",
      "pattern": "\\b(XS|S|M|L|XL|XXL|XXXL|2XL|3XL|\\d{2}|\\d{2}\\.\\d)\\b",
      "source": "title"
    },
    "color": {
      "method": "regex",
      "pattern": "(Black|White|Blue|Red|Green|Yellow|Orange|Purple|Pink|Gray|Grey|Navy|Khaki|Beige|Brown)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "material": {
      "method": "regex",
      "pattern": "(Cotton|Polyester|Denim|Blend|Lycra|Viscose)",
      "source": "title",
      "flags": "IGNORECASE"
    }
  }
}

