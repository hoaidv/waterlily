{
  "category": "iPhones",
  "website": "newegg",
  "search_query": "iPhone cell phone",
  "listing_selectors": {
    "product_container": "div.item-cell",
    "product_link": "a.item-title"
  },
  "product_selectors": {
    "title": [
      {"selector": "h1.product-title"}
    ],
    "price": [
      {"selector": "li.price-current"},
      {"selector": "span.price-current"}
    ],
    "rating": [
      {"selector": "a[class*='rating']", "attribute": "title"}
    ],
    "images": [
      {"selector": "img.product-view-img-original", "attribute": "src"}
    ]
  },
  "attributes": {
    "brand": {
      "method": "regex",
      "pattern": "(Apple|iPhone)",
      "source": "title",
      "default": "Apple"
    },
    "model": {
      "method": "regex",
      "pattern": "(iPhone\\s*\\d+[a-zA-Z]*(?:\\s*Pro\\s*Max)?(?:\\s*Plus)?)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "storage": {
      "method": "regex",
      "pattern": "(\\d+)\\s*GB(?!\\s*RAM)",
      "source": "title"
    },
    "color": {
      "method": "regex",
      "pattern": "(Black|White|Blue|Red|Green|Purple|Pink|Gold|Silver|Gray|Grey|Titanium|Starlight|Midnight|Graphite|Sierra Blue|Alpine Green)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "condition": {
      "method": "regex",
      "pattern": "(New|Open Box|Refurbished|Used)",
      "source": "title",
      "flags": "IGNORECASE",
      "default": "New"
    },
    "carrier": {
      "method": "regex",
      "pattern": "(Unlocked|AT&T|Verizon|T-Mobile|Sprint)",
      "source": "title",
      "flags": "IGNORECASE"
    }
  }
}

