{
  "category": "iPhones",
  "website": "flipkart",
  "search_query": "iPhone mobile",
  "listing_selectors": {
    "product_link": "a[href*='/p/']"
  },
  "product_selectors": {
    "title": [
      {"selector": "span.B_NuCI"},
      {"selector": "h1.yhB1nd"}
    ],
    "price": [
      {"selector": "div._30jeq3"},
      {"selector": "div._1_WHN1"}
    ],
    "rating": [
      {"selector": "div._3LWZlK", "regex": "(\\d+\\.?\\d*)"}
    ],
    "images": [
      {"selector": "img._396cs4", "attribute": "src"}
    ]
  },
  "attributes": {
    "brand": {
      "method": "regex",
      "pattern": "(Apple|iPhone)",
      "source": "title",
      "default": "Apple"
    },
    "model": {
      "method": "regex",
      "pattern": "(iPhone\\s*\\d+[a-zA-Z]*(?:\\s*Pro\\s*Max)?(?:\\s*Plus)?)",
      "source": "title",
      "flags": "IGNORECASE"
    },
    "storage": {
      "method": "regex",
      "pattern": "(\\d+)\\s*GB(?:\\s*Storage|\\s*ROM)?",
      "source": "title"
    },
    "ram": {
      "method": "regex",
      "pattern": "(\\d+)\\s*GB\\s*RAM",
      "source": "title"
    },
    "color": {
      "method": "regex",
      "pattern": "\\(([^,)]+),\\s*\\d+\\s*GB\\)",
      "source": "title",
      "group": 1,
      "fallback": {
        "method": "regex",
        "pattern": "(Black|White|Blue|Red|Green|Purple|Pink|Gold|Silver|Gray|Grey|Titanium|Starlight|Midnight|Graphite|Sierra Blue|Alpine Green)",
        "flags": "IGNORECASE"
      }
    },
    "screen_size": {
      "method": "regex",
      "pattern": "(\\d+\\.?\\d*)\\s*(?:inch|cm)",
      "source": "title",
      "flags": "IGNORECASE"
    }
  }
}

